{
  "name": "cars",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "node scripts/check-conflicts.mjs",
    "build": "next build",
    "pages:build": "cross-env GITHUB_PAGES=true NEXT_PUBLIC_REPO=car next build",
    "pages:post": "mkdir -p out && cp -f public/404.html out/404.html 2>/dev/null || true && touch out/.nojekyll",
    "start": "next start",
    "lint": "eslint",
    "scrub:pii": "node scripts/scrub-pii.js",
    "preview:html": "http-server ./static-html -p 5173 -c-1",
    "preview:html:open": "npx open-cli http://localhost:5173/ || start http://localhost:5173/ || xdg-open http://localhost:5173/",
    "start:html": "npm run preview:html"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@vimeo/player": "^2.29.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.15",
    "lucide-react": "^0.544.0",
    "motion": "^12.23.15",
    "next": "^15.5.3",
    "next-intl": "^4.3.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.8",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "http-server": "^14.1.1",
    "open-cli": "^7.2.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
